
<!--start-shoes-->
<div class="shoes">
	<div class="container">
		<div class="product-one">
			{{#each get4}}
			<div class="col-md-3 product-left">
				<div class="p-one simpleCart_shelfItem">
					<a href="/san-pham/{{id}}-{{alias}}.html">
						<img src="/product/{{image}}" alt="" />
						<div class="mask">
							<span>Xem ngay</span>
						</div>
					</a>
					<h4>{{name}}</h4>
					{{#if ../user}}
					<p><a class="item_add_cart" href="#" data-id="{{ id }}" data-price="{{ price }}"  data-name="{{ name }}"><i></i> <span class=" item_price">{{#fmNum price}}{{/fmNum}}</span></a></p>
					{{else}}
					<p><a href="/login"><i></i> <span class=" item_price">{{#fmNum price}}{{/fmNum}}</span></a></p>
					{{/if}}

				</div>
			</div>
			{{/each}}
			<div class="clearfix"> </div>
		</div>
		<div class="product-one">
			{{#each get4Offset}}
			<div class="col-md-3 product-left">
				<div class="p-one simpleCart_shelfItem">
					<a href="/san-pham/{{id}}-{{alias}}.html">
						<img src="/product/{{image}}" alt="" />
						<div class="mask">
							<span>Xem ngay</span>
						</div>
					</a>
					<h4>{{name}}</h4>
					{{#if ../user}}
					<p><a class="item_add_cart" href="#" data-id="{{ id }}" data-price="{{ price }}"  data-name="{{ name }}"><i></i> <span class=" item_price">{{#fmNum price}}{{/fmNum}}</span></a></p>
					{{else}}
					<p><a href="/login"><i></i> <span class=" item_price">{{#fmNum price}}{{/fmNum}}</span></a></p>
					{{/if}}
				</div>
			</div>
			{{/each}}
			<div class="clearfix"> </div>
		</div>
	</div>
</div>
