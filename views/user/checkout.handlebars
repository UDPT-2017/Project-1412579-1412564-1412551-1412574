
	<!--start-breadcrumbs-->
	<div class="breadcrumbs">
		<div class="container">
			<div class="breadcrumbs-main">
				<ol class="breadcrumb">
					<li><a href="index.html">Home</a></li>
					<li class="active">Checkout</li>
				</ol>
			</div>
		</div>
	</div>
	<!--end-breadcrumbs-->
	<!--start-ckeckout-->
	<div class="ckeckout">
		<div class="container">
			<div class="ckeckout-top">
			<div class=" cart-items heading">
			 <h3>My Shopping Bag (3)</h3>

			<div class="in-check" >
				<ul class="unit">
					<li><span>Item</span></li>
					<li><span>Product Name</span></li>
					<li><span>Unit Price</span></li>
					<li><span>Quantity</span></li>
					<li> </li>
					<div class="clearfix"> </div>
				</ul>
				{{#each items as |item| }}
					<ul class="cart-header">
						<div class="close1 remove_item" data-id={{ item.product_id }}> </div>
							<li class="ring-in"><a href="single.html" ><img src="images/s-1.jpg" class="img-responsive" alt=""></a>
							</li>
							<li><span>{{ item.product_name }}</span></li>
							<li><span>$ {{ item.price }}</span></li>
							<li><span id="quantity-{{ item.product_id }}">{{ item.quantity }}</span></li>
							<li> <a href="single.html" class="add-cart cart-check item_add_cart_checkout" data-id="{{ item.product_id }}" data-price="{{ item.price }}"  data-name="{{ item.product_name }}" >ADD TO CART</a></li>
						<div class="clearfix"> </div>
					</ul>
				{{/each}}
			</div>
			</div>
		 </div>
		</div>
	</div>
	<!--end-ckeckout-->
